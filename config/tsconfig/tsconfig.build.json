{
  // https://www.typescriptlang.org/ja/tsconfig
  "extends": "./tsconfig.type-check.json",
  "compilerOptions": {
    "noEmit": false,

    // Emit
    "downlevelIteration": true,
    "importHelpers": true,
    "importsNotUsedAsValues": "remove",
    "newLine": "lf",
    "noEmitOnError": true,

    // Projects
    "incremental": false
  }
}
