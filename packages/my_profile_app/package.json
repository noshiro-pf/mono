{
  "name": "my_profile_app",
  "version": "0.1.0",
  "license": "MIT",
  "author": "noshiro",
  "scripts": {
    "_script_formatchanged": "node ${npm_package_config_scripts_dir}/format_changed",
    "_script_formatglob": "node ${npm_package_config_scripts_dir}/format_glob",
    "prebuild": "rimraf ${npm_package_config_dist_dir}",
    "build": "webpack --config \"${npm_package_config_webpack_prod_config}\"",
    "postbuild": "cpx \"${npm_package_config_public_dir}/**\" \"${npm_package_config_dist_dir}\"",
    "compilewebpackconfig": "tsc --project ${npm_package_config_tsconfig_for_webpack}",
    "postcompilewebpackconfig": "prettier --write \"./${npm_package_config_config_dir}/*.js\"",
    "fmt": "yarn _script_formatchanged master",
    "fmtall": "yarn _script_formatglob \"./src/**/*.{ts,tsx}\"",
    "lint": "eslint --config \"${npm_package_config_eslintrc_path}\" src/ --ext .js,.jsx,.ts,.tsx",
    "serve": "npx http-server \"${npm_package_config_dist_dir}\"",
    "setup": "yarn compilewebpackconfig",
    "start": "webpack-dev-server --config \"${npm_package_config_webpack_dev_config}\"",
    "test": "jest --config \"${npm_package_config_jest_config}\"",
    "testw": "yarn test --watch"
  },
  "config": {
    "config_dir": "config",
    "dist_dir": "build",
    "eslintrc_path": ".eslintrc.json",
    "jest_config": "jest.config.json",
    "public_dir": "public",
    "scripts_dir": "../../scripts",
    "tsconfig_for_webpack": "config/tsconfig.reactconfig.json",
    "webpack_dev_config": "config/webpack.dev.js",
    "webpack_prod_config": "config/webpack.prod.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-media": "^1.10.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  }
}
