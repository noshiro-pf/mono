{
  "name": "@noshiro/chain-rule-slides",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "author": "noshiro",
  "sideEffects": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "clean": "run-p clean:**",
    "clean:build": "rimraf dist",
    "fb:deploy": "yarn z:fb:deploy:wireit",
    "fb:init": "firebase init",
    "fb:login": "firebase login",
    "preview": "vite preview",
    "start": "vite",
    "z:fb:deploy:impl": "firebase deploy",
    "z:fb:deploy:wireit": "wireit"
  },
  "dependencies": {
    "reveal.js": "^5.1.0"
  },
  "devDependencies": {
    "@noshiro/eslint-configs": "*"
  },
  "wireit": {
    "z:fb:deploy:wireit": {
      "command": "yarn z:fb:deploy:impl",
      "dependencies": [
        "build"
      ]
    },
    "build": {
      "command": "vite build",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*"
      ],
      "output": [
        "dist/**",
        ".tsbuildinfo"
      ]
    }
  }
}
