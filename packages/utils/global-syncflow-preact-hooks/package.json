{
  "name": "@noshiro/global-syncflow-preact-hooks",
  "version": "1.0.0",
  "license": "MIT",
  "author": "noshiro",
  "sideEffects": true,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "run-p build:**",
    "build:cjs": "yarn z:wireit:build:cjs",
    "build:esm": "yarn z:wireit:build:esm",
    "clean": "run-p clean:**",
    "clean:build": "rimraf cjs esm",
    "clean:eslintcache": "rimraf .eslintcache",
    "clean:wireit": "rimraf .wireit",
    "fmt": "yarn z:cmd:prettier .",
    "fmt:src": "yarn z:cmd:prettier ./src",
    "lint": "yarn z:wireit:lint",
    "lint:fix": "yarn z:wireit:lint:fix",
    "setup": "yarn z:cmd:setup:seq",
    "tsc": "yarn z:no-wireit:type-check",
    "tscw": "yarn z:cmd:type-check --watch",
    "z:cmd:eslint": "eslint src/ --ext .ts,.tsx --color --cache --cache-location .eslintcache",
    "z:cmd:jest": "jest --config config/jest.config.json --passWithNoTests",
    "z:cmd:prettier": "prettier --ignore-path ../../../.prettierignore  --write",
    "z:cmd:setup:1": "node ./scripts/gen.js",
    "z:cmd:setup:2": "yarn fmt:src",
    "z:cmd:setup:3": "yarn build",
    "z:cmd:setup:seq": "run-s z:cmd:setup:1 z:cmd:setup:2 z:cmd:setup:3",
    "z:cmd:tsc-incremental": "tsc --incremental --pretty",
    "z:cmd:type-check": "tsc --noEmit",
    "z:no-wireit:build:cjs": "yarn z:cmd:tsc-incremental --project config/tsconfig.cjs.json",
    "z:no-wireit:build:esm": "yarn z:cmd:tsc-incremental --project config/tsconfig.esm.json",
    "z:no-wireit:lint": "yarn z:cmd:eslint",
    "z:no-wireit:lint:fix": "yarn z:cmd:eslint --fix",
    "z:no-wireit:type-check": "yarn z:cmd:type-check",
    "z:wireit:build:cjs": "wireit",
    "z:wireit:build:esm": "wireit",
    "z:wireit:lint": "wireit",
    "z:wireit:lint:fix": "wireit",
    "z:wireit:type-check": "wireit"
  },
  "dependencies": {
    "@noshiro/syncflow-preact-hooks": "*",
    "@noshiro/ts-utils": "*"
  },
  "devDependencies": {
    "@noshiro/ts-type-utils": "*"
  },
  "wireit": {
    "z:wireit:build:cjs": {
      "command": "yarn z:no-wireit:build:cjs",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [
        "cjs/**",
        ".tsbuildinfo"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:build:esm": {
      "command": "yarn z:no-wireit:build:esm",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [
        "esm/**",
        ".tsbuildinfo"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:lint": {
      "command": "yarn z:no-wireit:lint",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "src/**/*.ts",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:lint:fix": {
      "command": "yarn z:no-wireit:lint:fix",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "src/**/*.ts",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "src/**/*.ts"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:type-check": {
      "command": "yarn z:no-wireit:type-check",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    }
  }
}
