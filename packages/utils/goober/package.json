{
  "name": "@noshiro/goober",
  "version": "2.0.33",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/goober.js",
  "module": "dist/goober.esm.js",
  "source": "src/index.js",
  "types": "goober.d.ts",
  "typings": "./goober.d.ts",
  "scripts": {
    "fmt": "prettier --ignore-path ../../../.prettierignore  --write .",
    "tsc": "yarn type-check",
    "type-check": "wireit"
  },
  "peerDependencies": {
    "csstype": "^2.6.2"
  },
  "wireit": {
    "type-check": {
      "command": "tsc --noEmit",
      "files": [
        "src/**/*.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    }
  }
}
