{
  "name": "@noshiro/solid-styled-components",
  "version": "0.23.11",
  "description": "Styled Components for Solid (forked)",
  "license": "MIT",
  "author": "noshiro",
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test:types": "tsc --project tsconfig.test.json",
    "tsc": "yarn z:wireit:type-check",
    "tscw": "yarn z:cmd:type-check --watch",
    "z:cmd:type-check": "tsc --noEmit",
    "z:no-wireit:type-check": "yarn z:cmd:type-check",
    "z:wireit:type-check": "wireit"
  },
  "dependencies": {
    "@noshiro/ts-utils": "*",
    "csstype": "^3.0.3",
    "goober": "^2.0.15"
  },
  "devDependencies": {
    "@noshiro/ts-type-utils": "*"
  },
  "peerDependencies": {
    "solid-js": "^0.23.11"
  },
  "wireit": {
    "z:wireit:type-check": {
      "command": "yarn z:no-wireit:type-check",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    }
  }
}
