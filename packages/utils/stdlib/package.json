{
  "name": "@noshiro/stdlib",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "autofix": "wireit",
    "fmt": "prettier --ignore-path ./.prettierignore  --write .",
    "gen": "./gen.sh"
  },
  "wireit": {
    "autofix": {
      "command": "eslint dist/ --ext .d.ts --fix --color --cache --cache-location .eslintcache",
      "files": [
        "dist/**/*.d.ts",
        ".eslintignore",
        ".eslintrc.cjs",
        "tsconfig.json",
        "../../../config/tsconfig/tsconfig.base.nolib.json",
        "../../../config/eslintrc/**"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    }
  }
}
