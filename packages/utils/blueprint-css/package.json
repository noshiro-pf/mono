{
  "name": "@noshiro/blueprint-css",
  "version": "1.0.0",
  "license": "MIT",
  "author": "noshiro",
  "sideEffects": false,
  "main": "blueprint-min.css",
  "style": "blueprint-min.css",
  "files": [
    "blueprint-min.css",
    "blueprint-icons.css",
    "resources/icons/*"
  ],
  "scripts": {
    "fmt": "prettier --ignore-path ../../../.prettierignore  --write .",
    "tsc": "yarn type-check",
    "type-check": "wireit"
  },
  "devDependencies": {
    "normalize.css": "^8.0.1"
  },
  "peerDependencies": {
    "normalize.css": "^8.0.1"
  },
  "wireit": {
    "type-check": {
      "command": "tsc --noEmit",
      "files": [
        "src/**/*.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    }
  }
}
