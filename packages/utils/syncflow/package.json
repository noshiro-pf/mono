{
  "name": "@noshiro/syncflow",
  "version": "1.1.1",
  "private": false,
  "description": "Reactive Programming Library for TypeScript/JavaScript",
  "license": "MIT",
  "author": "noshiro",
  "sideEffects": false,
  "main": "cjs/src/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "package.json",
    "cjs",
    "esm"
  ],
  "scripts": {
    "build": "run-p build:**",
    "build:cjs": "yarn z:wireit:build:cjs",
    "build:esm": "yarn z:wireit:build:esm",
    "clean": "run-p clean:**",
    "clean:build": "rimraf cjs esm",
    "clean:eslintcache": "rimraf .eslintcache",
    "clean:wireit": "rimraf .wireit",
    "fmt": "yarn z:cmd:prettier",
    "gi": "run-p gi:**",
    "gi:src": "yarn z:no-wireit:gi:src",
    "gi:test": "yarn z:no-wireit:gi:test",
    "lint": "yarn z:wireit:lint",
    "lint:fix": "yarn z:wireit:lint:fix",
    "pub": "yarn z:cmd:publish",
    "setup": "yarn build",
    "start": "node cjs/test/preview-main.js",
    "test": "yarn z:wireit:test",
    "test:detectOpenHandles": "yarn z:cmd:jest:detectOpenHandles",
    "testw": "yarn z:cmd:jest --watch",
    "tsc": "yarn z:no-wireit:type-check",
    "tscw": "yarn z:cmd:type-check --watch",
    "z:cmd:eslint": "eslint \"./{src,test}/**/*\" --ext .ts,.tsx --color --cache --cache-location .eslintcache",
    "z:cmd:gi": "bash ../../../scripts/index_ts_generator.sh",
    "z:cmd:jest": "jest --config config/jest.config.json --passWithNoTests",
    "z:cmd:jest:detectOpenHandles": "yarn z:cmd:jest --detectOpenHandles",
    "z:cmd:prettier": "prettier --ignore-path ../../../.prettierignore  --write .",
    "z:cmd:publish": "yarn publish --no-git-tag-version --access=public",
    "z:cmd:tsc-incremental": "tsc --incremental --pretty",
    "z:cmd:type-check": "tsc --noEmit",
    "z:no-wireit:build:cjs": "yarn z:cmd:tsc-incremental --project config/tsconfig.cjs.json",
    "z:no-wireit:build:esm": "yarn z:cmd:tsc-incremental --project config/tsconfig.esm.json",
    "z:no-wireit:gi:src": "yarn z:cmd:gi ./src --min-depth 1",
    "z:no-wireit:gi:test": "yarn z:cmd:gi ./test/cases --min-depth 1",
    "z:no-wireit:lint": "yarn z:cmd:eslint",
    "z:no-wireit:lint:fix": "yarn z:cmd:eslint --fix",
    "z:no-wireit:test": "yarn z:cmd:jest",
    "z:no-wireit:type-check": "yarn z:cmd:type-check",
    "z:wireit:build:cjs": "wireit",
    "z:wireit:build:esm": "wireit",
    "z:wireit:gi:src": "wireit",
    "z:wireit:gi:test": "wireit",
    "z:wireit:lint": "wireit",
    "z:wireit:lint:fix": "wireit",
    "z:wireit:test": "wireit",
    "z:wireit:type-check": "wireit"
  },
  "dependencies": {
    "@noshiro/ts-utils": "*",
    "@noshiro/ts-utils-additional": "*"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.1",
    "@noshiro/ts-type-utils": "*",
    "@tsconfig/docusaurus": "^1.0.6"
  },
  "peerDependencies": {
    "tslib": "^2.4.0",
    "typescript": "^4.9.3"
  },
  "wireit": {
    "z:wireit:build:cjs": {
      "command": "yarn z:no-wireit:build:cjs",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [
        "cjs/**",
        ".tsbuildinfo"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:build:esm": {
      "command": "yarn z:no-wireit:build:esm",
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [
        "esm/**",
        ".tsbuildinfo"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:gi:src": {
      "command": "yarn z:no-wireit:gi:src",
      "clean": false,
      "files": [
        "../../../scripts/index_ts_generator.sh",
        "src/**"
      ],
      "output": [
        "src/*/**/index.ts"
      ],
      "packageLocks": []
    },
    "z:wireit:gi:test": {
      "command": "yarn z:no-wireit:gi:test",
      "clean": false,
      "files": [
        "../../../scripts/index_ts_generator.sh",
        "test/cases/**"
      ],
      "output": [
        "test/cases/**/index.ts"
      ],
      "packageLocks": []
    },
    "z:wireit:lint": {
      "command": "yarn z:no-wireit:lint",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:lint:fix": {
      "command": "yarn z:no-wireit:lint:fix",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "src/**/*.ts",
        "test/**/*.ts",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "src/**/*.ts"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:test": {
      "command": "yarn z:no-wireit:test",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "config/jest.config.json",
        "src/**"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:type-check": {
      "command": "yarn z:no-wireit:type-check",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    }
  }
}
