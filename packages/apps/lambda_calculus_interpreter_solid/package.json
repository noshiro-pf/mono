{
  "name": "@mono/lambda-interpreter-app-solid",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean:build",
    "build": "solid-scripts build",
    "clean": "npm-run-all clean:*",
    "clean:build": "rimraf ${npm_package_config_dist_dir}",
    "clean:config": "rimraf config/**/*.js scripts/**/*.js",
    "fmt": "prettier --write .",
    "lint": "eslint src/ --ext .ts,.tsx",
    "setup": "echo 'no setup script'",
    "start": "solid-scripts start",
    "test": "echo 'no test script'",
    "testw": "solid-scripts test"
  },
  "config": {
    "dist_dir": "build",
    "scripts_dir": "../../../scripts"
  },
  "browserslist": [
    "Chrome 74",
    "Firefox 63",
    "Safari 11",
    "Edge 17",
    "Node 10"
  ],
  "dependencies": {
    "@mono/ts-utils": "*",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@mono/solid-styled-components": "*",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "solid-js": "^0.23.11",
    "solid-scripts": "0.0.50",
    "typescript": "4.1.3"
  }
}
