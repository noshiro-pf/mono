{
  "name": "@noshiro/color-demo-app",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "author": "noshiro",
  "browser": "src/main.tsx",
  "scripts": {
    "build": "yarn z:bundle:wireit",
    "build:no-minify": "yarn z:bundle:wireit --minify false",
    "clean": "run-p clean:**",
    "clean:build": "rimraf build",
    "clean:eslintcache": "rimraf .eslintcache",
    "clean:firebase": "rimraf .firebase",
    "clean:wireit": "rimraf .wireit/*/cache",
    "cy:open": "cypress open",
    "cy:record": "cypress run --record --key ${CYPRESS_RECORD_KEY}",
    "cy:run:chrome": "cypress run --headed --browser chrome",
    "cy:run:firefox": "cypress run --headed --browser firefox",
    "fb:deploy": "yarn z:fb:deploy:wireit",
    "fb:deploy:hosting": "yarn z:fb:deploy:hosting:wireit",
    "fb:init": "firebase init",
    "fb:login": "firebase login",
    "fb:login:ci": "firebase login:ci",
    "fmt": "yarn zz:cmd:prettier .",
    "gi": "yarn z:gi:wireit",
    "lint": "run-s z:lint:src:wireit z:lint:cy:wireit",
    "lint:fix": "run-s z:lint:fix:src:wireit z:lint:fix:cy:wireit",
    "preview": "yarn zz:cmd:vite:preview",
    "serve": "firebase serve",
    "start": "run-p start:**",
    "start:dev-server": "yarn zz:cmd:vite --port 5180",
    "test": "yarn z:test:wireit",
    "testw": "yarn zz:cmd:vitest watch",
    "tsc": "yarn z:type-check:wireit",
    "tscw": "yarn z:type-check:impl --watch",
    "z:bundle:impl": "yarn zz:cmd:vite build",
    "z:bundle:wireit": "wireit",
    "z:dev-server:impl": "yarn zz:cmd:vite --port 5180",
    "z:dev-server:wireit": "wireit",
    "z:fb:deploy:hosting:impl": "firebase deploy --only hosting",
    "z:fb:deploy:hosting:wireit": "wireit",
    "z:fb:deploy:impl": "firebase deploy",
    "z:fb:deploy:wireit": "wireit",
    "z:gi:impl": "yarn zz:cmd:gi ./src --min-depth 1 --ignore assets",
    "z:gi:wireit": "wireit",
    "z:lint:cy:impl": "yarn zz:cmd:eslint:cy",
    "z:lint:cy:wireit": "wireit",
    "z:lint:fix:cy:impl": "yarn zz:cmd:eslint:cy --fix",
    "z:lint:fix:cy:wireit": "wireit",
    "z:lint:fix:src:impl": "yarn zz:cmd:eslint:src --fix",
    "z:lint:fix:src:wireit": "wireit",
    "z:lint:src:impl": "yarn zz:cmd:eslint:src",
    "z:lint:src:wireit": "wireit",
    "z:setup": "run-p z:setup:gen-global-dts",
    "z:setup:gen-global-dts": "node ./scripts/gen-global-dts.js",
    "z:test:impl": "yarn zz:cmd:vitest run",
    "z:test:wireit": "wireit",
    "z:type-check:impl": "tsc --noEmit",
    "z:type-check:wireit": "wireit",
    "zz:cmd:eslint": "eslint --ext .ts,.tsx --color",
    "zz:cmd:eslint:cy": "yarn zz:cmd:eslint cypress/ --cache --cache-location cypress/.eslintcache",
    "zz:cmd:eslint:src": "yarn zz:cmd:eslint src/ --cache --cache-location ./.eslintcache",
    "zz:cmd:gi": "bash ../../../scripts/index_ts_generator.sh",
    "zz:cmd:prettier": "prettier --cache --cache-strategy content --ignore-path ../../../.prettierignore --write",
    "zz:cmd:vite": "vite --config config/vite.config.ts",
    "zz:cmd:vite:preview": "yarn zz:cmd:vite preview",
    "zz:cmd:vitest": "vitest --config config/vitest.config.ts --passWithNoTests"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/material": "^5.11.6",
    "@noshiro/react-mui-utils": "*",
    "@noshiro/react-utils": "*",
    "@noshiro/syncflow": "*",
    "@noshiro/syncflow-react-hooks": "*",
    "@noshiro/ts-utils": "*",
    "@noshiro/ts-utils-additional": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@noshiro/global-emotion-react": "*",
    "@noshiro/global-emotion-styled": "*",
    "@noshiro/global-react": "*",
    "@noshiro/global-react-utils": "*",
    "@noshiro/global-syncflow": "*",
    "@noshiro/global-syncflow-react-hooks": "*",
    "@noshiro/global-ts-utils": "*",
    "@noshiro/ts-type-utils": "*"
  },
  "wireit": {
    "z:dev-server:wireit": {
      "command": "yarn z:dev-server:impl",
      "service": true
    },
    "z:bundle:wireit": {
      "command": "yarn z:bundle:impl",
      "dependencies": [
        "z:type-check:wireit"
      ],
      "files": [
        "public/**",
        "src/**/*",
        "tsconfig.json",
        "config/globals.ts",
        "config/vite.config.ts",
        "../../../config/tsconfig/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [
        "build/**"
      ]
    },
    "z:fb:deploy:wireit": {
      "command": "yarn z:fb:deploy:impl",
      "dependencies": [
        "z:bundle:wireit"
      ]
    },
    "z:fb:deploy:hosting:wireit": {
      "command": "yarn z:fb:deploy:hosting:impl",
      "dependencies": [
        "z:bundle:wireit"
      ]
    },
    "z:gi:wireit": {
      "command": "yarn z:gi:impl",
      "clean": false,
      "files": [
        "../../../scripts/index_ts_generator.sh",
        "src/**"
      ],
      "output": [
        "src/*/**/index.ts"
      ],
      "packageLocks": []
    },
    "z:lint:src:wireit": {
      "command": "yarn z:lint:src:impl",
      "files": [
        "src/**/*.{ts,tsx}",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": []
    },
    "z:lint:fix:src:wireit": {
      "command": "yarn z:lint:fix:src:impl",
      "files": [
        "src/**/*.{ts,tsx}",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "src/**/*.{ts,tsx}"
      ]
    },
    "z:lint:cy:wireit": {
      "command": "yarn z:lint:cy:impl",
      "files": [
        "cypress/**/*.ts",
        "cypress/.eslintrc.cjs",
        "cypress/tsconfig.json",
        "package.json",
        ".eslintignore",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": []
    },
    "z:lint:fix:cy:wireit": {
      "command": "yarn z:lint:fix:cy:impl",
      "files": [
        "cypress/**/*.ts",
        "cypress/.eslintrc.cjs",
        "cypress/tsconfig.json",
        "package.json",
        ".eslintignore",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "cypress/**/*.ts"
      ]
    },
    "z:test:wireit": {
      "command": "yarn z:test:impl",
      "files": [
        "config/global.ts",
        "config/tsconfig.test.json",
        "config/vitest.config.ts",
        "src/**"
      ]
    },
    "z:type-check:wireit": {
      "command": "yarn z:type-check:impl",
      "dependencies": [
        "z:setup"
      ],
      "files": [
        "src/**/*.{ts,tsx}",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": []
    }
  }
}
