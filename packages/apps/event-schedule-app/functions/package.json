{
  "name": "@noshiro/event-schedule-app-functions",
  "version": "5.1.0",
  "private": false,
  "license": "MIT",
  "author": "noshiro",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn z:wireit:build",
    "clean": "run-p clean:**",
    "clean:build": "rimraf dist",
    "clean:eslintcache": "rimraf .eslintcache",
    "clean:firebase": "rimraf .firebase",
    "clean:wireit": "rimraf .wireit",
    "fb:config:get": "firebase functions:config:get",
    "fb:config:set": "firebase functions:config:set",
    "fb:config:setapikey": "firebase functions:config:set slack.apikey=",
    "fb:deploy": "firebase deploy --only functions",
    "fb:logs": "firebase functions:log",
    "fb:shell": "firebase functions:shell",
    "fb:showlog": "firebase functions:log",
    "fmt": "yarn z:cmd:prettier",
    "gi": "yarn z:wireit:gi",
    "lint": "yarn z:wireit:lint",
    "lint:fix": "yarn z:wireit:lint:fix",
    "setup": "yarn build",
    "start:emulators": "firebase emulators:start --only functions",
    "start:shell": "yarn shell",
    "tsc": "yarn z:wireit:type-check",
    "tscw": "yarn z:cmd:type-check --watch",
    "z:cmd:eslint": "eslint src/ --ext .ts,.tsx --color --cache --cache-location .eslintcache",
    "z:cmd:gi": "bash ../../../../scripts/index_ts_generator.sh ./src --min-depth 1",
    "z:cmd:prettier": "prettier --ignore-path ../../../../.prettierignore  --write .",
    "z:no-wireit:build": "tsc --incremental --pretty --project tsconfig.json",
    "z:no-wireit:fb:deploy": "firebase deploy",
    "z:no-wireit:gi": "yarn z:cmd:gi",
    "z:no-wireit:lint": "yarn z:cmd:eslint --color",
    "z:no-wireit:lint:fix": "yarn z:cmd:eslint --fix --color",
    "z:no-wireit:test": "yarn z:cmd:vitest run",
    "z:no-wireit:type-check": "tsc --noEmit",
    "z:wireit:build": "wireit",
    "z:wireit:fb:deploy": "wireit",
    "z:wireit:gi": "wireit",
    "z:wireit:lint": "wireit",
    "z:wireit:lint:fix": "wireit",
    "z:wireit:type-check": "wireit"
  },
  "dependencies": {
    "@noshiro/event-schedule-app-shared": "*",
    "@noshiro/fast-deep-equal": "*",
    "@noshiro/io-ts": "*",
    "@noshiro/ts-utils": "*",
    "firebase-admin": "^10.2.0",
    "firebase-functions": "^3.21.0",
    "nodemailer": "^6.4.16"
  },
  "devDependencies": {
    "@noshiro/ts-type-utils": "*"
  },
  "engines": {
    "node": "16"
  },
  "wireit": {
    "z:wireit:build": {
      "command": "yarn z:no-wireit:build",
      "clean": "if-file-deleted",
      "dependencies": [
        "../../event-schedule-app-shared:build:cjs"
      ],
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../../config/tsconfig/**",
        "../../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [
        "cjs/**",
        ".tsbuildinfo"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:fb:deploy": {
      "command": "yarn z:no-wireit:fb:deploy",
      "dependencies": [
        "z:wireit:build"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:gi": {
      "command": "yarn z:no-wireit:gi",
      "clean": false,
      "files": [
        "../../../../scripts/index_ts_generator.sh",
        "src/**"
      ],
      "output": [
        "src/*/**/index.ts"
      ],
      "packageLocks": []
    },
    "z:wireit:lint": {
      "command": "yarn z:no-wireit:lint",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "src/**/*.ts",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../../package.json",
        "../../../../config/tsconfig/**",
        "../../../../config/eslintrc/**",
        "../../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:lint:fix": {
      "command": "yarn z:no-wireit:lint:fix",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "src/**/*.ts",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../../package.json",
        "../../../../config/tsconfig/**",
        "../../../../config/eslintrc/**",
        "../../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "src/**/*.ts"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:type-check": {
      "command": "yarn z:no-wireit:type-check",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../../config/tsconfig/**",
        "../../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    }
  }
}
