{
  "name": "@noshiro/event-schedule-app",
  "version": "5.0.1",
  "private": false,
  "license": "MIT",
  "author": "noshiro",
  "sideEffects": true,
  "type": "module",
  "browser": "src/main.tsx",
  "scripts": {
    "build": "wireit",
    "build:functions": "yarn workspace @noshiro/event-schedule-app-functions build",
    "build:no-minify": "yarn build --minify false",
    "clean": "run-p clean:**",
    "clean:build": "rimraf build",
    "clean:eslintcache": "rimraf .eslintcache",
    "clean:firebase": "rimraf .firebase",
    "clean:wireit": "rimraf .wireit/**",
    "cy:open": "yarn workspace @noshiro/event-schedule-app-cypress cy:open",
    "cy:record": "yarn workspace @noshiro/event-schedule-app-cypress cy:record",
    "cy:run:chrome": "yarn workspace @noshiro/event-schedule-app-cypress cy:run:chrome",
    "cy:run:firefox": "yarn workspace @noshiro/event-schedule-app-cypress cy:run:firefox",
    "fb": "firebase",
    "fb:deploy": "wireit",
    "fb:deploy:functions": "wireit",
    "fb:deploy:hosting": "wireit",
    "fb:export": "wireit",
    "fb:init": "firebase init",
    "fb:login": "firebase login",
    "fb:login:ci": "firebase login:ci",
    "fb:write": "node ./scripts/write_firestore",
    "fmt": "yarn zz:cmd:prettier .",
    "gi": "wireit",
    "lint": "run-p lint:src lint:cy",
    "lint:cy": "wireit",
    "lint:fix": "run-p lint:fix:src lint:fix:cy",
    "lint:fix:cy": "wireit",
    "lint:fix:src": "wireit",
    "lint:src": "wireit",
    "preview": "yarn zz:cmd:vite:preview",
    "serve": "firebase serve",
    "start": "run-p start:build-functions start:dev-server start:emulators",
    "start:build-functions": "cd functions && yarn watch",
    "start:dev-server": "wireit",
    "start:emulators": "wireit",
    "start:emulators:e2e": "firebase emulators:start --only functions,firestore --import firebase-emulator-exports --export-on-exit",
    "test": "wireit",
    "testw": "yarn zz:cmd:vitest watch",
    "tsc": "yarn type-check",
    "tscw": "tsc --noEmit --watch",
    "type-check": "run-p type-check:src type-check:cy",
    "type-check:cy": "wireit",
    "type-check:src": "wireit",
    "z:setup": "run-p z:setup:build-deps z:setup:gen-global-dts",
    "z:setup:build-deps": "wireit",
    "z:setup:gen-global-dts": "node ./scripts/gen-global-dts.mjs",
    "z:setup:scripts": "wireit",
    "z:setup:service-account-key": "touch ./scripts/service-account-key.json",
    "zz:cmd:eslint": "ESLINT_USE_FLAT_CONFIG=true eslint",
    "zz:cmd:eslint:cy": "yarn zz:cmd:eslint --config ./cypress/eslint.config.js './cypress/**/*.ts' --cache --cache-location ./cypress/.eslintcache",
    "zz:cmd:eslint:print-config": "yarn zz:cmd:eslint --print-config src/main.tsx",
    "zz:cmd:eslint:src": "yarn zz:cmd:eslint --config eslint.config.js './src/**/*' --cache --cache-location ./src/.eslintcache",
    "zz:cmd:prettier": "prettier --cache --cache-strategy content --ignore-path ../../../.prettierignore --write",
    "zz:cmd:vite": "vite --config configs/vite.config.ts",
    "zz:cmd:vite:preview": "yarn zz:cmd:vite preview",
    "zz:cmd:vitest": "vitest --config ./configs/vite.config.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^5.0.1",
    "@blueprintjs/datetime": "^5.0.1",
    "@blueprintjs/icons": "^5.9.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.5",
    "@noshiro/deep-object-diff": "*",
    "@noshiro/event-schedule-app-shared": "*",
    "@noshiro/fast-deep-equal": "*",
    "@noshiro/io-ts": "*",
    "@noshiro/react-utils": "*",
    "@noshiro/syncflow": "*",
    "@noshiro/syncflow-react-hooks": "*",
    "@noshiro/tiny-router-observable": "*",
    "@noshiro/tiny-router-react-hooks": "*",
    "@noshiro/ts-utils": "*",
    "@noshiro/ts-utils-additional": "*",
    "firebase": "^10.6.0",
    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "url-regex": "^5.0.0"
  },
  "devDependencies": {
    "@noshiro/eslint-configs": "*",
    "@noshiro/global-emotion-react": "*",
    "@noshiro/global-emotion-styled": "*",
    "@noshiro/global-react": "*",
    "@noshiro/global-react-utils": "*",
    "@noshiro/global-syncflow": "*",
    "@noshiro/global-syncflow-react-hooks": "*",
    "@noshiro/global-tiny-router-observable": "*",
    "@noshiro/global-tiny-router-react-hooks": "*",
    "@noshiro/global-ts-utils": "*",
    "@noshiro/mono-scripts": "*",
    "@noshiro/ts-type-utils": "*",
    "@noshiro/ts-utils": "*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.24"
  },
  "wireit": {
    "type-check:src": {
      "dependencies": [
        "z:setup",
        "z:setup:service-account-key"
      ],
      "command": "tsc --noEmit",
      "files": [
        "./**/*.{mjs,mts,js,ts,jsx,tsx,d.mts,d.ts}",
        "./tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**"
      ],
      "output": []
    },
    "type-check:cy": {
      "command": "tsc --noEmit -p ./cypress/tsconfig.json",
      "files": [
        "./cypress/**/*.{mjs,mts,js,ts,jsx,tsx,d.mts,d.ts}",
        "./cypress/tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**"
      ],
      "output": []
    },
    "gi": {
      "command": "bash ../../../scripts/bash/index_ts_generator.sh ./src --min-depth 1 --ignore assets",
      "files": [
        "../../../scripts/bash/index_ts_generator.sh",
        "./src/**/*.{mjs,mts,js,ts,jsx,tsx,d.mts,d.ts}"
      ],
      "clean": false,
      "output": [
        "src/*/**/index.ts"
      ],
      "packageLocks": []
    },
    "build": {
      "dependencies": [
        "type-check"
      ],
      "command": "yarn zz:cmd:vite build",
      "files": [
        "./public/**",
        "./src/**/*.{mjs,mts,js,ts,jsx,tsx,d.mts,d.ts}",
        "./configs/vite.config.ts",
        "../../../package.json",
        "../../../config/tsconfig/**"
      ],
      "output": [
        "build/**"
      ]
    },
    "test": {
      "command": "yarn zz:cmd:vitest run",
      "files": [
        "./src/**/*.{mjs,mts,js,ts,jsx,tsx,d.mts,d.ts}",
        "./configs/vite.config.ts",
        "./configs/tsconfig.test.json",
        "../../../package.json",
        "../../../config/tsconfig/**"
      ],
      "output": []
    },
    "lint:src": {
      "command": "yarn zz:cmd:eslint:src",
      "files": [
        "./src/**/*.{mjs,mts,js,ts,jsx,tsx,d.mts,d.ts}",
        "./eslint.config.js",
        "./package.json",
        "./tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**"
      ],
      "output": []
    },
    "lint:fix:src": {
      "command": "yarn zz:cmd:eslint:src --fix",
      "files": [
        "./src/**/*.{mjs,mts,js,ts,jsx,tsx,d.mts,d.ts}",
        "./eslint.config.js",
        "./package.json",
        "./tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**"
      ],
      "clean": false,
      "output": [
        "./src/**/*.{mjs,mts,js,ts,jsx,tsx,d.mts,d.ts}"
      ]
    },
    "lint:cy": {
      "command": "yarn zz:cmd:eslint:cy",
      "files": [
        "./cypress/**/*.ts",
        "./cypress/eslint.config.js",
        "./cypress/tsconfig.json",
        "./package.json",
        "../../../package.json",
        "../../../config/tsconfig/**"
      ],
      "output": []
    },
    "lint:fix:cy": {
      "command": "yarn zz:cmd:eslint:cy --fix",
      "files": [
        "./cypress/**/*.ts",
        "./cypress/eslint.config.js",
        "./cypress/tsconfig.json",
        "./package.json",
        "../../../package.json",
        "../../../config/tsconfig/**"
      ],
      "clean": false,
      "output": [
        "./cypress/**/*.ts"
      ]
    },
    "fb:deploy": {
      "dependencies": [
        "build"
      ],
      "command": "firebase deploy"
    },
    "fb:deploy:hosting": {
      "dependencies": [
        "build"
      ],
      "command": "firebase deploy --only hosting"
    },
    "start:dev-server": {
      "service": true,
      "command": "yarn zz:cmd:vite --port 5180"
    },
    "z:setup:build-deps": {
      "dependencies": [
        "../event-schedule-app-shared:build"
      ],
      "command": "echo 'build deps'"
    },
    "fb:deploy:functions": {
      "dependencies": [
        "./functions:build"
      ],
      "command": "firebase deploy --only functions"
    },
    "start:emulators": {
      "dependencies": [
        "./functions:build"
      ],
      "service": true,
      "command": "firebase emulators:start --only functions,firestore,pubsub --import firebase-emulator-exports --export-on-exit"
    },
    "fb:export": {
      "command": "node ./scripts/export-firestore > ./scripts/firestore_backup.json && prettier --cache --cache-strategy content --write ./scripts/firestore_backup.json"
    },
    "z:setup:scripts": {
      "dependencies": [
        "z:setup:service-account-key",
        "../event-schedule-app-shared:build"
      ],
      "command": "tsc --project ./configs/tsconfig.scripts.json",
      "files": [
        "./configs/tsconfig.scripts.json",
        "./scripts/**/*.ts",
        "../../../config/tsconfig/**"
      ],
      "clean": false,
      "output": [
        "./scripts/**/*.js",
        "!./scripts/gen-global-dts.mjs"
      ]
    }
  }
}
