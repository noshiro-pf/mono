{
  "name": "@noshiro/event-schedule-app",
  "version": "5.0.1",
  "private": false,
  "license": "MIT",
  "author": "noshiro",
  "browser": "src/main.tsx",
  "scripts": {
    "build": "yarn z:bundle:wireit",
    "build:no-minify": "yarn z:bundle:wireit --minify false",
    "clean": "run-p clean:**",
    "clean:build": "rimraf build",
    "clean:eslintcache": "rimraf .eslintcache",
    "clean:firebase": "rimraf .firebase",
    "clean:wireit": "rimraf .wireit/*/cache",
    "cy:open": "cypress open",
    "cy:record": "cypress run --record --key ${CYPRESS_RECORD_KEY}",
    "cy:run:chrome": "cypress run --headed --browser chrome",
    "cy:run:firefox": "cypress run --headed --browser firefox",
    "fb:deploy": "yarn z:fb:deploy:wireit",
    "fb:deploy:functions": "yarn z:fb:deploy:functions:wireit",
    "fb:deploy:hosting": "yarn z:fb:deploy:hosting:wireit",
    "fb:export": "yarn z:fb:export:wireit",
    "fb:init": "firebase init",
    "fb:login": "firebase login",
    "fb:login:ci": "firebase login:ci",
    "fb:write": "node ./scripts/write_firestore",
    "fmt": "yarn zz:cmd:prettier .",
    "gi": "yarn z:gi:wireit",
    "lint": "run-s z:lint:src:wireit z:lint:cy:wireit",
    "lint:fix": "run-s z:lint:fix:src:wireit z:lint:fix:cy:wireit",
    "preview": "yarn zz:cmd:vite:preview",
    "serve": "firebase serve",
    "start": "run-p start:**",
    "start:build-functions": "cd functions && yarn watch",
    "start:dev-server": "yarn z:dev-server:wireit",
    "start:emulators": "yarn z:fb:emulators:start:wireit",
    "test": "yarn z:test:wireit",
    "testw": "yarn zz:cmd:vitest watch",
    "tsc": "yarn z:type-check:wireit",
    "tscw": "yarn z:type-check:impl --watch",
    "z:bundle:impl": "yarn zz:cmd:vite build",
    "z:bundle:wireit": "wireit",
    "z:dev-server:impl": "yarn zz:cmd:vite --port 5180",
    "z:dev-server:wireit": "wireit",
    "z:fb:deploy:functions:wireit": "wireit",
    "z:fb:deploy:hosting:impl": "firebase deploy --only hosting",
    "z:fb:deploy:hosting:wireit": "wireit",
    "z:fb:deploy:impl": "firebase deploy",
    "z:fb:deploy:wireit": "wireit",
    "z:fb:emulators:start:impl": "firebase emulators:start --only functions,firestore --import firebase-emulator-exports --export-on-exit",
    "z:fb:emulators:start:wireit": "wireit",
    "z:fb:export:impl": "node ./scripts/export-firestore > ./scripts/firestore_backup.json && prettier --cache --cache-strategy content --write ./scripts/firestore_backup.json",
    "z:fb:export:wireit": "wireit",
    "z:gi:impl": "yarn zz:cmd:gi ./src --min-depth 1 --ignore assets",
    "z:gi:wireit": "wireit",
    "z:lint:cy:impl": "yarn zz:cmd:eslint:cy",
    "z:lint:cy:wireit": "wireit",
    "z:lint:fix:cy:impl": "yarn zz:cmd:eslint:cy --fix",
    "z:lint:fix:cy:wireit": "wireit",
    "z:lint:fix:src:impl": "yarn zz:cmd:eslint:src --fix",
    "z:lint:fix:src:wireit": "wireit",
    "z:lint:src:impl": "yarn zz:cmd:eslint:src",
    "z:lint:src:wireit": "wireit",
    "z:setup": "run-p z:setup:build-deps z:setup:gen-global-dts",
    "z:setup:build-deps": "wireit",
    "z:setup:gen-global-dts": "node ./scripts/gen-global-dts.mjs",
    "z:setup:scripts:impl": "tsc --project config/tsconfig.scripts.json",
    "z:setup:scripts:wireit": "wireit",
    "z:setup:service-account-key": "touch ./scripts/service-account-key.json",
    "z:test:impl": "yarn zz:cmd:vitest run",
    "z:test:wireit": "wireit",
    "z:type-check:impl": "tsc --noEmit --project config/tsconfig.src.json",
    "z:type-check:wireit": "wireit",
    "zz:cmd:eslint": "eslint --ext .ts,.tsx --color",
    "zz:cmd:eslint:cy": "yarn zz:cmd:eslint cypress/ --cache --cache-location cypress/.eslintcache",
    "zz:cmd:eslint:src": "yarn zz:cmd:eslint src/ --cache --cache-location ./.eslintcache",
    "zz:cmd:gi": "bash ../../../scripts/index_ts_generator.sh",
    "zz:cmd:prettier": "prettier --cache --cache-strategy content --ignore-path ../../../.prettierignore --write",
    "zz:cmd:vite": "vite --config config/vite.config.ts",
    "zz:cmd:vite:preview": "yarn zz:cmd:vite preview",
    "zz:cmd:vitest": "vitest --config config/vitest.config.ts --passWithNoTests"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^4.20.1",
    "@blueprintjs/datetime": "^4.4.13",
    "@blueprintjs/datetime2": "^1.0.0",
    "@blueprintjs/icons": "^4.13.0",
    "@blueprintjs/popover2": "^1.12.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@noshiro/deep-object-diff": "*",
    "@noshiro/event-schedule-app-shared": "*",
    "@noshiro/fast-deep-equal": "*",
    "@noshiro/io-ts": "*",
    "@noshiro/react-utils": "*",
    "@noshiro/syncflow": "*",
    "@noshiro/syncflow-react-hooks": "*",
    "@noshiro/tiny-router-observable": "*",
    "@noshiro/tiny-router-react-hooks": "*",
    "@noshiro/ts-utils": "*",
    "@noshiro/ts-utils-additional": "*",
    "date-fns": "^2.30.0",
    "firebase": "^9.22.1",
    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "url-regex": "^5.0.0"
  },
  "devDependencies": {
    "@noshiro/global-emotion-react": "*",
    "@noshiro/global-emotion-styled": "*",
    "@noshiro/global-react": "*",
    "@noshiro/global-react-utils": "*",
    "@noshiro/global-syncflow": "*",
    "@noshiro/global-syncflow-react-hooks": "*",
    "@noshiro/global-tiny-router-observable": "*",
    "@noshiro/global-tiny-router-react-hooks": "*",
    "@noshiro/global-ts-utils": "*",
    "@noshiro/ts-type-utils": "*",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.4"
  },
  "wireit": {
    "z:setup:build-deps": {
      "command": "echo 'build deps'",
      "dependencies": [
        "../event-schedule-app-shared:build"
      ]
    },
    "z:dev-server:wireit": {
      "command": "yarn z:dev-server:impl",
      "service": true
    },
    "z:bundle:wireit": {
      "command": "yarn z:bundle:impl",
      "dependencies": [
        "z:type-check:wireit"
      ],
      "files": [
        "public/**",
        "src/**/*",
        "tsconfig.json",
        "config/globals.ts",
        "config/vite.config.ts",
        "../../../config/tsconfig/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [
        "build/**"
      ]
    },
    "z:fb:deploy:wireit": {
      "command": "yarn z:fb:deploy:impl",
      "dependencies": [
        "z:bundle:wireit",
        "./functions:build"
      ]
    },
    "z:fb:deploy:functions:wireit": {
      "command": "yarn z:fb:deploy:functions:impl",
      "dependencies": [
        "./functions:build"
      ]
    },
    "z:fb:deploy:hosting:wireit": {
      "command": "yarn z:fb:deploy:hosting:impl",
      "dependencies": [
        "z:bundle:wireit"
      ]
    },
    "z:fb:export:wireit": {
      "command": "yarn z:fb:export:impl",
      "dependencies": [
        "z:setup:scripts:wireit"
      ]
    },
    "z:fb:emulators:start:wireit": {
      "command": "yarn z:fb:emulators:start:impl",
      "dependencies": [
        "./functions:z:setup"
      ],
      "service": true
    },
    "z:gi:wireit": {
      "command": "yarn z:gi:impl",
      "clean": false,
      "files": [
        "../../../scripts/index_ts_generator.sh",
        "src/**"
      ],
      "output": [
        "src/*/**/index.ts"
      ],
      "packageLocks": []
    },
    "z:lint:src:wireit": {
      "command": "yarn z:lint:src:impl",
      "files": [
        "src/**/*.{ts,tsx}",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": []
    },
    "z:lint:fix:src:wireit": {
      "command": "yarn z:lint:fix:src:impl",
      "files": [
        "src/**/*.{ts,tsx}",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "src/**/*.{ts,tsx}"
      ]
    },
    "z:lint:cy:wireit": {
      "command": "yarn z:lint:cy:impl",
      "files": [
        "cypress/**/*.ts",
        "cypress/.eslintrc.cjs",
        "cypress/tsconfig.json",
        "package.json",
        ".eslintignore",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": []
    },
    "z:lint:fix:cy:wireit": {
      "command": "yarn z:lint:fix:cy:impl",
      "files": [
        "cypress/**/*.ts",
        "cypress/.eslintrc.cjs",
        "cypress/tsconfig.json",
        "package.json",
        ".eslintignore",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "cypress/**/*.ts"
      ]
    },
    "z:setup:scripts:wireit": {
      "command": "yarn z:setup:scripts:impl",
      "dependencies": [
        "z:setup:service-account-key",
        "../event-schedule-app-shared:build"
      ],
      "files": [
        "config/tsconfig.scripts.json",
        "scripts/**/*.ts",
        "../../../config/tsconfig/**"
      ],
      "clean": false,
      "output": [
        "scripts/**/*.js",
        "!scripts/gen-global-dts.mjs"
      ]
    },
    "z:test:wireit": {
      "command": "yarn z:test:impl",
      "files": [
        "config/global.ts",
        "config/tsconfig.test.json",
        "config/vitest.config.ts",
        "src/**"
      ]
    },
    "z:type-check:wireit": {
      "command": "yarn z:type-check:impl",
      "dependencies": [
        "z:setup"
      ],
      "files": [
        "src/**/*.{ts,tsx}",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": []
    }
  }
}
