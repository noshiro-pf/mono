{
  "name": "@noshiro/event-schedule-app-functions",
  "version": "5.0.0",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "_script_genindex": "bash ${npm_package_config_root_dir}/scripts/index_ts_generator.sh",
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf dist",
    "deploy": "firebase deploy --only functions",
    "fb": "firebase",
    "firebase": "firebase",
    "getconfig": "firebase functions:config:get",
    "gi": "yarn _script_genindex ./src --min-depth 1 --ignore assets",
    "lint": "eslint src/ --ext .ts",
    "lint:deps": "yarn lint --no-eslintrc --config ${npm_package_config_root_dir}/config/eslintrc/.eslintrc-no-extraneous-dependency.js",
    "lint:fix": "yarn lint --fix",
    "logs": "firebase functions:log",
    "preserve": "yarn build",
    "serve": "firebase emulators:start --only functions",
    "setconfig": "firebase functions:config:set",
    "preshell": "yarn build",
    "shell": "firebase functions:shell",
    "showlog": "firebase functions:log",
    "start": "yarn shell",
    "tsc": "tsc --noEmit",
    "type-check": "tsc --noEmit"
  },
  "config": {
    "root_dir": "../../../.."
  },
  "dependencies": {
    "@noshiro/event-schedule-app-shared": "5.0.2",
    "@noshiro/fast-deep-equal": "1.0.1",
    "@noshiro/ts-utils": "1.2.0",
    "firebase-admin": "^9.12.0",
    "firebase-functions": "^3.15.7",
    "nodemailer": "^6.4.16"
  },
  "devDependencies": {
    "@noshiro/ts-type-utils": "*"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
