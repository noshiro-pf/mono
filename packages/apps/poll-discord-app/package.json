{
  "name": "@noshiro/poll-discord-app",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn z:bundle:wireit",
    "clean": "run-p clean:**",
    "clean:build": "rimraf dist",
    "clean:eslintcache": "rimraf .eslintcache",
    "clean:wireit": "rimraf .wireit/*/cache",
    "fb:init": "firebase init",
    "fb:login": "firebase login",
    "fb:login:ci": "firebase login:ci",
    "fmt": "yarn zz:cmd:prettier .",
    "gi": "yarn z:gi:wireit",
    "lint": "yarn z:lint:src:wireit",
    "lint:fix": "yarn z:lint:fix:src:wireit",
    "scp": "yarn zz:cmd:gcloud:scp:wireit",
    "start:dev": "node ./dist/bot.dev.cjs",
    "start:prd": "node ./dist/bot.prd.cjs",
    "tsc": "yarn z:type-check:wireit",
    "tscw": "yarn z:type-check:impl --watch",
    "z:bundle:impl": "run-p zz:cmd:bundle:*:impl",
    "z:bundle:wireit": "wireit",
    "z:gi:impl": "yarn zz:cmd:gi ./src --min-depth 1 --ignore assets",
    "z:gi:wireit": "wireit",
    "z:lint:fix:src:impl": "yarn zz:cmd:eslint:src --fix",
    "z:lint:fix:src:wireit": "wireit",
    "z:lint:src:impl": "yarn zz:cmd:eslint:src",
    "z:lint:src:wireit": "wireit",
    "z:type-check:impl": "tsc --noEmit",
    "z:type-check:wireit": "wireit",
    "zz:cmd:bundle:dev:impl": "node ./scripts/bundle.dev.mjs",
    "zz:cmd:bundle:prd:impl": "node ./scripts/bundle.prd.mjs",
    "zz:cmd:eslint": "eslint --ext .ts,.tsx --color",
    "zz:cmd:eslint:src": "yarn zz:cmd:eslint src/ --cache --cache-location ./.eslintcache",
    "zz:cmd:gcloud:scp:impl": "gcloud compute scp ./bot.*.cjs ./.env.* e2-micro-1:/home/noshiro --zone=us-central1-a",
    "zz:cmd:gcloud:scp:wireit": "wireit",
    "zz:cmd:gi": "bash ../../../scripts/index_ts_generator.sh",
    "zz:cmd:prettier": "prettier --cache --cache-strategy content --ignore-path ../../../.prettierignore --write",
    "zz:cmd:publish": "yarn publish --no-git-tag-version --access=public",
    "zz:cmd:tsc-incremental": "tsc --incremental --pretty"
  },
  "dependencies": {
    "@noshiro/io-ts": "*",
    "@noshiro/ts-utils": "*",
    "@noshiro/ts-utils-additional": "*",
    "discord.js": "^14.12.1",
    "dotenv": "^16.0.3",
    "firebase": "^10.4.0"
  },
  "devDependencies": {
    "@noshiro/global-ts-utils": "*",
    "@noshiro/ts-type-utils": "*"
  },
  "engines": {
    "node": ">=16.6.x"
  },
  "wireit": {
    "zz:cmd:gcloud:scp:wireit": {
      "command": "yarn zz:cmd:gcloud:scp:impl",
      "dependencies": [
        "z:type-check:wireit",
        "build"
      ]
    },
    "z:bundle:wireit": {
      "command": "yarn z:bundle:impl",
      "clean": true,
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "./scripts/**/*",
        "../../../config/tsconfig/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [
        "./dist/**/*"
      ]
    },
    "z:gi:wireit": {
      "command": "yarn z:gi:impl",
      "clean": false,
      "files": [
        "../../../scripts/index_ts_generator.sh",
        "src/**"
      ],
      "output": [
        "src/*/**/index.ts"
      ],
      "packageLocks": []
    },
    "z:lint:src:wireit": {
      "command": "yarn z:lint:src:impl",
      "files": [
        "src/**/*.ts",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": []
    },
    "z:lint:fix:src:wireit": {
      "command": "yarn z:lint:fix:src:impl",
      "files": [
        "src/**/*.ts",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "src/**/*.ts"
      ]
    },
    "z:type-check:wireit": {
      "command": "yarn z:type-check:impl",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../../utils/stdlib/stdlib.d.ts",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": []
    }
  }
}
