{
  "name": "@noshiro/mahjong-calculator-app-cypress",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "cy:open": "cypress open",
    "cy:record": "cypress run --record --key ${CYPRESS_RECORD_KEY}",
    "cy:run:chrome": "cypress run --headed --browser chrome ",
    "cy:run:firefox": "cypress run --headed --browser firefox ",
    "start:dev-server": "yarn workspace @noshiro/mahjong-calculator-app start:dev-server",
    "start:emulators": "yarn workspace @noshiro/mahjong-calculator-app start:emulators",
    "start:emulators:e2e": "yarn workspace @noshiro/mahjong-calculator-app start:emulators:e2e"
  }
}
