{
  "name": "@noshiro/algo-app",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "author": "noshiro",
  "scripts": {
    "build": "yarn z:wireit:bundle",
    "build:no-minify": "yarn z:wireit:bundle --minify false",
    "clean": "run-p clean:**",
    "clean:build": "rimraf build",
    "clean:eslintcache": "rimraf .eslintcache",
    "clean:firebase": "rimraf .firebase",
    "clean:wireit": "rimraf .wireit",
    "cy:open": "cypress open",
    "cy:record": "cypress run --record --key ${CYPRESS_RECORD_KEY}",
    "cy:run:chrome": "cypress run --headed --browser chrome",
    "cy:run:firefox": "cypress run --headed --browser firefox",
    "fb:deploy": "yarn z:wireit:fb:deploy",
    "fb:deploy:hosting": "yarn z:wireit:fb:deploy:hosting",
    "fb:init": "firebase init",
    "fb:login": "firebase login",
    "fb:login:ci": "firebase login:ci",
    "fmt": "yarn z:cmd:prettier",
    "gi": "yarn z:wireit:gi",
    "lint": "run-s z:wireit:lint:*",
    "lint:fix": "run-p z:wireit:lint:fix:*",
    "preview": "yarn z:cmd:vite:preview",
    "serve": "firebase serve",
    "start": "yarn start:dev-server",
    "start:dev-server": "yarn z:cmd:vite --port 5180",
    "test": "yarn z:wireit:test",
    "testw": "yarn z:cmd:vitest watch",
    "tsc": "yarn z:wireit:type-check",
    "tscw": "yarn z:no-wireit:type-check --watch",
    "z:cmd:eslint": "eslint --ext .ts,.tsx",
    "z:cmd:eslint:cy": "yarn z:cmd:eslint cypress/ --cache --cache-location cypress/.eslintcache",
    "z:cmd:eslint:src": "yarn z:cmd:eslint src/ --cache --cache-location ./.eslintcache",
    "z:cmd:prettier": "prettier --ignore-path ../../../.prettierignore  --write .",
    "z:cmd:vite": "vite --config config/vite.config.ts",
    "z:cmd:vite:preview": "yarn z:cmd:vite preview",
    "z:cmd:vitest": "vitest --config config/vitest.config.ts --passWithNoTests",
    "z:no-wireit:bundle": "yarn z:cmd:vite build",
    "z:no-wireit:fb:deploy": "firebase deploy",
    "z:no-wireit:fb:deploy:hosting": "firebase deploy --only hosting",
    "z:no-wireit:gi": "bash ../../../scripts/index_ts_generator.sh ./src --min-depth 1 --ignore assets",
    "z:no-wireit:lint:cy": "yarn z:cmd:eslint:cy --color",
    "z:no-wireit:lint:fix:cy": "yarn z:cmd:eslint:cy --fix --color",
    "z:no-wireit:lint:fix:src": "yarn z:cmd:eslint:src --fix --color",
    "z:no-wireit:lint:src": "yarn z:cmd:eslint:src --color",
    "z:no-wireit:test": "yarn z:cmd:vitest run",
    "z:no-wireit:type-check": "tsc --noEmit",
    "z:wireit:bundle": "wireit",
    "z:wireit:fb:deploy": "wireit",
    "z:wireit:fb:deploy:hosting": "wireit",
    "z:wireit:gi": "wireit",
    "z:wireit:lint:cy": "wireit",
    "z:wireit:lint:fix:cy": "wireit",
    "z:wireit:lint:fix:src": "wireit",
    "z:wireit:lint:src": "wireit",
    "z:wireit:test": "wireit",
    "z:wireit:type-check": "wireit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@noshiro/fast-deep-equal": "*",
    "@noshiro/goober": "*",
    "@noshiro/io-ts": "*",
    "@noshiro/preact-utils": "*",
    "@noshiro/resize-observer-preact-hooks": "*",
    "@noshiro/syncflow": "*",
    "@noshiro/syncflow-preact-hooks": "*",
    "@noshiro/tiny-router-observable": "*",
    "@noshiro/tiny-router-preact-hooks": "*",
    "@noshiro/ts-utils": "*",
    "@noshiro/ts-utils-additional": "*",
    "firebase": "^9.0.2",
    "immer": "^9.0.7",
    "preact": "^10.4.7"
  },
  "devDependencies": {
    "@noshiro/global-goober": "*",
    "@noshiro/global-preact": "*",
    "@noshiro/global-preact-utils": "*",
    "@noshiro/global-syncflow": "*",
    "@noshiro/global-syncflow-preact-hooks": "*",
    "@noshiro/global-tiny-router-observable": "*",
    "@noshiro/global-tiny-router-preact-hooks": "*",
    "@noshiro/global-ts-utils": "*",
    "@noshiro/ts-type-utils": "*"
  },
  "wireit": {
    "z:wireit:bundle": {
      "command": "yarn z:no-wireit:bundle",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "public/**",
        "src/**/*",
        "tsconfig.json",
        "config/globals.ts",
        "config/vite.config.ts",
        "../../../config/tsconfig/**",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [
        "build/**"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:fb:deploy": {
      "command": "yarn z:no-wireit:fb:deploy",
      "dependencies": [
        "build"
      ]
    },
    "z:wireit:fb:deploy:hosting": {
      "command": "yarn z:no-wireit:fb:deploy:hosting",
      "dependencies": [
        "build"
      ]
    },
    "z:wireit:gi": {
      "command": "yarn z:no-wireit:gi",
      "clean": false,
      "files": [
        "../../../scripts/index_ts_generator.sh",
        "src/**"
      ],
      "output": [
        "src/*/**/index.ts"
      ],
      "packageLocks": []
    },
    "z:wireit:lint:src": {
      "command": "yarn z:no-wireit:lint:src",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "src/**/*.{ts,tsx}",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:lint:fix:src": {
      "command": "yarn z:no-wireit:lint:fix:src",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "src/**/*.{ts,tsx}",
        ".eslintignore",
        ".eslintrc.cjs",
        "package.json",
        "tsconfig.json",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "src/**/*.{ts,tsx}"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:lint:cy": {
      "command": "yarn z:no-wireit:lint:cy",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "cypress/**/*.ts",
        "cypress/.eslintrc.cjs",
        "cypress/tsconfig.json",
        "package.json",
        ".eslintignore",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:lint:fix:cy": {
      "command": "yarn z:no-wireit:lint:fix:cy",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "cypress/**/*.ts",
        "cypress/.eslintrc.cjs",
        "cypress/tsconfig.json",
        "package.json",
        ".eslintignore",
        "../../../package.json",
        "../../../config/tsconfig/**",
        "../../../config/eslintrc/**",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "clean": false,
      "output": [
        "cypress/**/*.ts"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:test": {
      "command": "yarn z:no-wireit:test",
      "dependencies": [
        "z:wireit:type-check"
      ],
      "files": [
        "config/global.ts",
        "config/tsconfig.test.json",
        "config/vitest.config.ts",
        "src/**"
      ],
      "packageLocks": [
        "yarn.lock"
      ]
    },
    "z:wireit:type-check": {
      "command": "yarn z:no-wireit:type-check",
      "files": [
        "src/**/*.{ts,tsx}",
        "tsconfig.json",
        "../../../config/tsconfig/**",
        "../../utils/ts-type-utils/ts-type-utils.d.ts"
      ],
      "output": [],
      "packageLocks": [
        "yarn.lock"
      ]
    }
  }
}
