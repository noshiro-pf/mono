{
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-packagejson"
  ],
  "semi": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "overrides": [
    {
      "files": ["source/temp/copied", "source/temp/copied/*.d.ts"],
      "options": {
        "printWidth": 320
      }
    }
  ]
}
