{
  "name": "rnjs2",
  "version": "1.0.0",
  "description": "Reactive Programming Library for TypeScript/JavaScript",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "webpack",
    "prestart": "rimraf dist",
    "start": "tsc --project tsconfig.dev.json && node dist/test/preview-main.js",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "pwd": "bash -c pwd",
    "genindex": "bash ../../scripts/test.sh",
    "gi": "yarn genindex ./src",
    "script_formatglob": "node ../../scripts-dist/format_glob",
    "script_formatchanged": "node ../../scripts-dist/format_changed",
    "fmtall": "yarn script_formatglob \"./src/**/*.{ts,tsx}\"",
    "fmtstaged": "yarn script_formatchanged",
    "fmt": "yarn script_formatchanged master",
    "genpipabletypedef": "export TARGET_FILE='./src/abstract_class/pipable.ts' && node ./scripts-dist/gen_pipable_type_def 12 $TARGET_FILE && yarn script_formatglob $TARGET_FILE",
    "test": "jest --watch --config jest.config.json"
  },
  "devDependencies": {
    "cross-env": "5.0.5",
    "@types/jest": "^26.0.8",
    "jest": "^26.2.2"
  }
}
