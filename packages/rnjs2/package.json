{
  "name": "rnjs2",
  "version": "1.0.0",
  "description": "Reactive Programming Library for TypeScript/JavaScript",
  "license": "MIT",
  "author": "noshiro",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "_script_formatchanged": "node ../../scripts_dist/format_changed",
    "_script_formatglob": "node ../../scripts_dist/format_glob",
    "_script_genindex": "bash ../../scripts/index_ts_generator.sh",
    "prebuild": "rimraf lib",
    "build": "webpack",
    "compilegenpipabletypedefscript": "tsc --project ./scripts/tsconfig.json",
    "compilewebpackconfig": "tsc ./webpack.config.ts && prettier --write ./webpack.config.js",
    "fmt": "yarn _script_formatchanged master",
    "fmtall": "yarn _script_formatglob \"./src/**/*.{ts,tsx}\"",
    "fmtstaged": "yarn _script_formatchanged",
    "genpipabletypedef": "node ./scripts_dist/gen_pipable_type_def 12 \"./src/abstract_class/pipable.ts\"",
    "postgenpipabletypedef": "yarn _script_formatglob \"./src/abstract_class/pipable.ts\"",
    "gi": "yarn _script_genindex ./src",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "prestart": "rimraf dist",
    "start": "tsc --project tsconfig.dev.json && node dist/test/preview-main.js",
    "test": "jest --watch --config jest.config.json"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.38",
    "@types/jest": "^26.0.9",
    "argparse": "^1.0.10",
    "cross-env": "5.0.5",
    "jest": "^26.2.2"
  }
}
